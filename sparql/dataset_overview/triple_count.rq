# How many triples are there in each split?
select ?tripleCountTotal ?tripleCountTraining ?tripleCountValidation ?tripleCountTest
where { 
    GRAPH <http://www.ontotext.com/explicit> {select (COUNT(*) AS ?tripleCountTotal) where { ?s ?p ?o} }
    GRAPH <http://bias.org/training-graph> {select (COUNT(*) AS ?tripleCountTraining) where { ?s ?p ?o} }
    GRAPH <http://bias.org/validation-graph> {select (COUNT(*) AS ?tripleCountValidation) where { ?s ?p ?o} }
    GRAPH <http://bias.org/test-graph> {select (COUNT(*) AS ?tripleCountTest) where { ?s ?p ?o} }
} 


# With percentages
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
select ?tripleCountTotal ?tripleCountTraining ((?tripleCountTraining/xsd:float(?tripleCountTotal)*100) AS ?trainingPercent) ?tripleCountValidation ((?tripleCountValidation/xsd:float(?tripleCountTotal)*100) AS ?validatinoPercent) ?tripleCountTest ((?tripleCountTest/xsd:float(?tripleCountTotal)*100) AS ?testPercent)
where { 
    GRAPH <http://www.ontotext.com/explicit> {select (COUNT(*) AS ?tripleCountTotal) where { ?s ?p ?o} }
    GRAPH <http://bias.org/training-graph> {select (COUNT(*) AS ?tripleCountTraining) where { ?s ?p ?o} }
    GRAPH <http://bias.org/validation-graph> {select (COUNT(*) AS ?tripleCountValidation) where { ?s ?p ?o} }
    GRAPH <http://bias.org/test-graph> {select (COUNT(*) AS ?tripleCountTest) where { ?s ?p ?o} }
} 