PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
SELECT ?relation (?s AS ?similarRelation) ((COUNT(*)/xsd:float(?tripleCount)) AS ?similarityScore)
WHERE {
    {
        SELECT ?relation (COUNT(*) AS ?tripleCount) WHERE { ?head ?relation ?tail . } GROUP BY ?relation 
    }
    ?h ?relation ?t .
    ?h ?s ?t .
	FILTER(?s != ?relation).
}
GROUP BY ?relation ?s ?tripleCount
HAVING ((COUNT(*) / xsd:float(?tripleCount)) > 0.5)